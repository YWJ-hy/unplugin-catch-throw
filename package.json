{
  "name": "unplugin-catch-throw",
  "type": "module",
  "version": "2.0.2",
  "packageManager": "pnpm@9.9.0",
  "description": "A plugin that throws exception in catch",
  "license": "MIT",
  "homepage": "https://github.com/YWJ-hy/unplugin-catch-throw#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YWJ-hy/unplugin-catch-throw.git"
  },
  "bugs": {
    "url": "https://github.com/YWJ-hy/unplugin-catch-throw/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vite-vue3": "pnpm run --filter vue-vite dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/parser": "^7.25.6",
    "@babel/traverse": "^7.25.6",
    "@babel/types": "^7.25.6",
    "@rollup/pluginutils": "^5.1.0",
    "magic-string": "^0.30.11",
    "unplugin": "^1.12.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@nuxt/kit": "^3.13.0",
    "@nuxt/schema": "^3.13.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.5.1",
    "bumpp": "^9.5.2",
    "chalk": "^5.3.0",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.1.4",
    "rollup": "^4.21.2",
    "typescript": "^5.5.4",
    "unbuild": "3.0.0-rc.7",
    "vite": "^5.4.2",
    "vitest": "^2.0.5",
    "webpack": "^5.94.0"
  }
}
